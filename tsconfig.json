{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ],
    "allowJs": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "outDir": "./tsfile",
    "rootDir": "./src",
    "declarationDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "module": "ES2015",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": false,
    "jsx": "preserve",
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@api/*": ["src/api/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@constance/*": ["src/constance/*"],
      "@store/*": ["src/store/*"],
      "@styles/*": ["src/styles/*"],
      "@views/*": ["src/views/*"],
      "@utils/*": ["src/utils/*"],
      "@router/*": ["src/router/*"],
    }
  },
  "include": [
    "./src",
  ],
  "files": [
    "./src/components/Ripple/index.tsx"
  ],
  "exclude": [
    "./tsfile",
    "./dist",
    "node_modules"
  ]
}